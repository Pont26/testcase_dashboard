{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "mydatabaseConnection": "Host=127.0.0.1;User ID=myuser;Password=mysecretpassword;Timeout=30;Database=mydatabase"
  },
  "Databases": {
    "mydatabase": {
      "NoPluralize": false,
      "UseDatabaseNames": false,
      "UseEFNaming": true,
      "PrefixNavigation": false
    }
  }
}